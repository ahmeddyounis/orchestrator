{
  "configVersion": 1,
  "thinkLevel": "L0",
  "budget": {
    "iter": 4,
    "tool": 6,
    "time": 600000
  },
  "providers": {
    "mock": {
      "type": "mock",
      "model": "mock"
    }
  },
  "defaults": {
    "planner": "mock",
    "executor": "mock",
    "reviewer": "mock"
  },
  "execution": {
    "allowDirtyWorkingTree": true,
    "noCheckpoints": false,
    "tools": {
      "enabled": false,
      "requireConfirmation": true,
      "allowlistPrefixes": [
        "pnpm test",
        "pnpm lint",
        "pnpm -r test",
        "pnpm -r lint",
        "pnpm -r build",
        "turbo run test",
        "turbo run build",
        "tsc",
        "vitest",
        "eslint",
        "prettier"
      ],
      "denylistPatterns": [
        "rm -rf",
        "mkfs",
        ":(){:|:&};:",
        "curl .*\\|\\s*sh"
      ],
      "allowNetwork": false,
      "timeoutMs": 600000,
      "maxOutputBytes": 10241024,
      "autoApprove": false,
      "interactive": true
    },
    "sandbox": {
      "mode": "none"
    }
  },
  "verification": {
    "enabled": true,
    "mode": "auto",
    "steps": [],
    "auto": {
      "enableLint": false,
      "enableTypecheck": true,
      "enableTests": true,
      "testScope": "targeted",
      "maxCommandsPerIteration": 3
    }
  }
}